---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Tian's Blog</title>
 <link href="http://tzheng.us/atom.xml" rel="self"/>
 <link href="http://tzheng.us/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://tzheng.us/</id>
 <author>
   <name>Tian</name>
   <email>naitgnehz@gmail.com</email>
 </author>

 {% for post in site.posts limit:20 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://tzheng.us{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://tzheng.us{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
